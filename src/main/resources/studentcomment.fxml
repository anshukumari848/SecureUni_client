            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="comment" layoutX="206.0" layoutY="492.0" prefHeight="30.0" prefWidth="412.0" promptText="Add comments">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextArea fx:id="commentinfo" layoutX="117.0" layoutY="120.0" prefHeight="322.0" prefWidth="632.0" />
      <Button fx:id="back" layoutX="705.0" layoutY="14.0" mnemonicParsing="false" onAction="#onbackclicked" prefHeight="30.0" prefWidth="69.0" text="Back">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
