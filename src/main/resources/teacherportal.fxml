      </Button>
      <Button fx:id="section" layoutX="74.0" layoutY="443.0" mnemonicParsing="false" onAction="#onsectionaction" prefHeight="25.0" prefWidth="128.0" text="Section">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="logout" layoutX="669.0" layoutY="530.0" mnemonicParsing="false" onAction="#onlogoutaction" style="-fx-background-color: #b20c0c;" text="LOG OUT" textFill="#e8e0e0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
